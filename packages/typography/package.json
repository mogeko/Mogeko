{
  "name": "@mogeko/typography",
  "version": "0.0.1",
  "description": "A typography plugin for Tailwind CSS",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "private": true,
  "keywords": [
    "typography",
    "tailwind"
  ],
  "author": "Mogeko <zhengjunyi@live.com>",
  "license": "MIT",
  "scripts": {
    "test": "vitest run",
    "cov": "vitest run --coverage",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "prettier --check .",
    "fmt": "prettier --write ."
  },
  "dependencies": {
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@vitest/coverage-v8": "^1.2.2",
    "prettier": "^3.2.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  }
}
